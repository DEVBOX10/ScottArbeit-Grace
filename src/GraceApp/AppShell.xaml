<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GraceApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GraceApp"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="grace diff">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:Diffs}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="grace diff 3">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:Diffsv3}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="grace">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
    </FlyoutItem>

</Shell>
